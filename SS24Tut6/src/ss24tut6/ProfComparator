import java.util.Comparator;
/**
 *
 * @author donat
 */
class ProfComparator implements Comparator<Professor>{

    @Override
    public int compare(Professor o1, Professor o2) {                            

        if (o1.getID() > o2.getID()) {
            return 1;
        } else if (o1.getID() == o2.getID()) {
            return 0;
        } else {
            return -1;
        }
    }
}

